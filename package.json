{
  "name": "react-visual-calendar-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm-run-all build:routes build:app build:server dev",
    "build:routes": "babel --presets es2017-node7 -s -D -d dist/routes routes",
    "build:app": "babel --presets es2017-node7 -s -D -d dist app.js",
    "build:server": "babel --presets es2017-node7 -s -D -d dist/bin bin",
    "help": "babel --help",
    "dev": "node dist/bin/www.js"
  },
  "dependencies": {
    "body-parser": "^1.16.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.0",
    "ejs": "^2.5.5",
    "express": "^4.14.1",
    "helmet": "^3.5.0",
    "moment": "^2.18.1",
    "morgan": "^1.7.0",
    "serve-favicon": "^2.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2017-node7": "^0.5.2",
    "jest": "^19.0.2",
    "npm-run-all": "^4.0.2"
  }
}
